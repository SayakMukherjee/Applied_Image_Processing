{
    "data_path": "../data",
    "log_path": "../logs",
    "save_path": "../outputs",
    "model_path": "../models",
    "dataset": "parisStreetView",
    "load_model": true,
    "seed": 40,
    "device": "cuda",
    "batch_size": 32,
    "learning_rate": 0.0002,
    "b1": 0.5,
    "b2": 0.999,
    "epochs": 20,
    "image_size": 128,
    "mask_size": 64,
    "val_interval": 5,
    "chkpoint_interval": 10,
    "use_style_content_loss": false,
    "content_layers": [
        "conv4_2"
    ],
    "style_layers": [
        "conv1_1",
        "conv2_1",
        "conv3_1",
        "conv4_1",
        "conv5_1"
    ]
}